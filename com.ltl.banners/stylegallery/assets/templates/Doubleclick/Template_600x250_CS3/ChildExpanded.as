package {	//import needed for all child files	import com.doubleclick.studio.proxy.Enabler;	//import needed for any files that call on the expand and collapse functions	import com.doubleclick.studio.expanding.ExpandingComponent;	import flash.display.MovieClip;	import flash.events.MouseEvent;	public class ChildExpanded extends MovieClip {		public function ChildExpanded():void {			//listener to call on the close button handler			close_btn.addEventListener(MouseEvent.CLICK, closeBTNHandler);			close_btn.buttonMode=true;			//listener to call on the background exit			expCon.addEventListener(MouseEvent.CLICK, bgHandler);			expCon.buttonMode=true;		}				private function closeBTNHandler(e:MouseEvent):void {			//closes the expanded panel. Basically it unloads it.			ExpandingComponent.collapse();			//track how many times users clicked the close button			Enabler.counter("Click to close");		}				private function bgHandler(e:MouseEvent):void {			//track how many times users clicked on the background			Enabler.exit("Background Click");		}	}}