package {	//import needed for all child files	import com.doubleclick.studio.proxy.Enabler;	//import needed for any files that call on the expand and collapse functions	import com.doubleclick.studio.expanding.ExpandingComponent;	import flash.display.MovieClip;	import flash.events.MouseEvent;	public class ChildCollapsed extends MovieClip {		public function ChildCollapsed() {			//expand the banner			colCTA.addEventListener(MouseEvent.MOUSE_UP, expandAd);			colCTA.buttonMode=true;		}		private function expandAd(e:MouseEvent):void {			//call this method to load the expanded panel into the parent file			ExpandingComponent.expand("Panel");			//track how many times users rolled over the CTA			Enabler.counter("Rollover to expand");		}	}}